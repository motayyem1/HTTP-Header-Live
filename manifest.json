{
	"manifest_version": 3,
	"name": "__MSG_extensionName__",
	"version": "0.6.5.3",
	"author": "Martin Antrag",
	"homepage_url":"https://github.com/Nitrama/HTTP-Header-Live/",
	"default_locale": "en",
	"options_ui": {
		"page": "options.html"
	},
	"icons": {
		"64": "icons/64.png"
	},
	"description": "__MSG_extensionDescription__",
	"permissions": [
		"storage",
		"activeTab",
		"webRequest",
		"tabs",
		"downloads",
		"sidePanel"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"side_panel": {
		"default_icon": "icons/icon.svg",
		"default_title" : "__MSG_extensionName__",
		"default_path": "HTTPHeaderMain.html"
	},
	"action": {
		"default_title": "__MSG_extensionName__",
		"default_icon": {
			"64": "icons/64.png"
		}
	},
	"background": {
		"service_worker": "background.js"
	}
}
